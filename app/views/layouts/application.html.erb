<!DOCTYPE html>
<html>
<head>
  <title>Project1</title>
  <%= stylesheet_link_tag    "custom.css", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
<div class="container">
	<div class="nav"> 
		<%= render 'layouts/header' %>
	</div>

	<div>
		<% if flash.any? %>
	
		<% flash.each do |info, msg| %>
		<%=  msg %>
		<% end %>
		<% end %>
		<%= yield %>

	<%= render 'layouts/footer' %>
	<div>
</div>
</body>
</html>
